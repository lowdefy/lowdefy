    "@lowdefy/block-tools": "3.1.1",
    "babel-jest": "26.6.3",
    "babel-loader": "8.2.2",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "6.4.0",
    "css-loader": "5.0.1",
    "html-webpack-plugin": "4.5.1",
    "jest": "26.6.3",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "style-loader": "2.0.0",
    "webpack": "5.14.0",
