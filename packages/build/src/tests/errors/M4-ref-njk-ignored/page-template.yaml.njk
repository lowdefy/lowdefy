# Nunjucks template with ~ignoreBuildCheck to suppress warning.
id: {{ pageId }}
type: Box
requests:
  - id: testRequest
    type: AxiosHttp
    connectionId: testApi
    payload:
      # ~ignoreBuildChecks suppresses the undefined state warning
      userData:
        _state: {{ blockId }}
        ~ignoreBuildChecks: true
    properties:
      url: /api/test
blocks:
  - id: content
    type: TextInput
