    "prepublishOnly": "pnpm build",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --passWithNoTests"
  },
  "devDependencies": {
    "@jest/globals": "28.1.3",
    "@swc/cli": "0.1.63",
    "@swc/core": "1.3.99",
    "@swc/jest": "0.2.29",
    "jest": "28.1.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
