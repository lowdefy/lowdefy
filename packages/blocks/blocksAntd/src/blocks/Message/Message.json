{
  "category": "display",
  "loading": false,
  "test": {
    "methods": [
      {
        "name": "open",
        "args": {}
      },
      {
        "name": "open",
        "args": {
          "status": "warning"
        }
      },
      {
        "name": "open",
        "args": {
          "content": "Args message content"
        }
      },
      {
        "name": "open",
        "args": {
