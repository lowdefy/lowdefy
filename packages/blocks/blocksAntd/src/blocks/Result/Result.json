{
  "category": "container",
  "loading": {
    "type": "Spinner",
    "properties": {
      "height": "100vh"
    }
  },
  "schema": {
    "properties": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "icon": {
          "type": ["string", "object"],
          "description": "Name of an Ant Design Icon or properties of an Icon block to customize icon to use as result image.",
          "docs": {
            "displayType": "icon"
          }
        },
        "status": {
          "type": "string",
          "enum": ["success", "error", "info", "warning", "404", "403", "500"],
          "default": "info",
          "description": "Status of the result. Determines image and color."
        },
        "subTitle": {
          "type": "string",
          "description": "Result subtitle or secondary text."
        },
        "title": {
          "type": "string",
          "description": "Result title or primary text."
        }
      }
    }
  }
}
